{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "es6": true,
  "env": {
    "browser": true,
    "node": true
  },
  "plugins" : [
    "import",
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "max-len": 0,
    "global-require": 0,
    "no-case-declarations": 0,
    "no-param-reassign": 1,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0,
    "comma-dangle": 0
  }
}
